{
  "name": "imagine-entertainment-website",
  "main": ".open-next/worker.js",
  "compatibility_date": "2024-09-23",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "invocation_logs": true
    }
  },
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "imagine-entertainment-website"
    }
  ],
  "workers_dev": false,
  "preview_urls": false,
  "routes": [
    { "pattern": "www.imaginesl.com/*", "zone_name": "imaginesl.com" }
  ],
  "triggers": {
    "crons": [
      "0 0 */3 * *"
    ]
  },
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
